_set_header Upgrade &lt;span class=&quot;built_in&quot;&gt;$http&lt;/span&gt;_upgrade&lt;span class=&quot;comment&quot;&gt;;&lt;/span&gt;&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;            proxy_set_header Connection &lt;span class=&quot;string&quot;&gt;&quot;upgrade&quot;&lt;/span&gt;&lt;span class=&quot;comment&quot;&gt;;&lt;/span&gt;&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;            proxy_connect_timeout &lt;span class=&quot;number&quot;&gt;7&lt;/span&gt;&lt;span class=&quot;keyword&quot;&gt;d&lt;/span&gt;&lt;span class=&quot;comment&quot;&gt;;&lt;/span&gt;&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;            proxy_send_timeout &lt;span class=&quot;number&quot;&gt;7&lt;/span&gt;&lt;span class=&quot;keyword&quot;&gt;d&lt;/span&gt;&lt;span class=&quot;comment&quot;&gt;;&lt;/span&gt;&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;            proxy_read_timeout &lt;span class=&quot;number&quot;&gt;7&lt;/span&gt;&lt;span class=&quot;keyword&quot;&gt;d&lt;/span&gt;&lt;span class=&quot;comment&quot;&gt;;&lt;/span&gt;&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;        &amp;#125;&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;		...&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;&amp;#125;&lt;/span&gt;&lt;br&gt;&lt;/pre&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/table&gt;&lt;/figure&gt;&lt;/p&gt;
